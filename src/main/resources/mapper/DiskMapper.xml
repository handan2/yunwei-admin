<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sss.yunweiadmin.mapper.DiskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sss.yunweiadmin.model.entity.Disk">
        <id column="id" property="id" />
        <result column="host_as_id" property="hostAsId" />
        <result column="sn" property="sn" />
        <result column="mode" property="mode" />
        <result column="status" property="status" />
        <result column="miji" property="miji" />
        <result column="capacity" property="capacity" />
        <result column="flag" property="flag" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, host_as_id, sn, mode, miji, status,capacity,flag, create_time
    </sql>

</mapper>
